RewriteEngine On
# RewriteRule ^(.*)/$ /$1.php [L]
# RewriteCond %{QUERY_STRING} .
# RewriteRule ^(.*)$ http://ww2.dev.kp.co.uk/$1? [R=301,L]
RewriteRule ^(.+)/$  /$1 [R=301,L] 
