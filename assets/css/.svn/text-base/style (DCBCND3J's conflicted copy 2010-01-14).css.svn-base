@charset "utf-8";
/* CSS Document */
html * {
	padding:0;
	margin:0;
}

body {
	background-color:#011036;
}

#wrapper {
	width:955px;
	margin-left:auto;
	margin-right:auto;
	background-image:url(../../images/wrapper_bg.jpg);
}

	#wrapper #header {
		height:184px;
		position:relative;
		z-index:9999;
	}
	
		#wrapper #header ul#navigation {
			position:relative;
			z-index:1;
			top:61px;
			background-image:url(../../images/assets.png);
			background-position: -138px -63px;
			width:955px;
			height:31px;
		}
		
		#wrapper #header ul#navigation li a {
			position:relative;
			display:block;
			top:0px;
			left:244px;
			width:95px;
			height:32px;
			background-image:url(../../images/assets.png);

			
		}
		
		#wrapper #header #logo {
			position:absolute;
			z-index:2;
			width:264px;
			height:183px;
			top:0px;
			left:345px;
			background-image:url(../../images/header/kp_logo.jpg);
		}
	
	#wrapper #content {
		height:435px;
		position:relative;
		z-index:500;
		margin-top:-100px;
	}
	
	#wrapper #footer {
		width:100%;
		height:184px;
		position:relative;
		z-index:1;
		margin-top:-45px;
	}
	
		#wrapper #footer .window_range {
			float:left;
			width:317px;
			height:184px;
			background-image:url(../../images/footer/window_range_bg.jpg);
		}
		
		#wrapper #footer .window_about {
			float:left;
			width:317px;
			height:184px;
			background-image:url(../../images/footer/window_aboutkp_bg.jpg);
		}

		#wrapper #footer .window_newsletter {
			float:left;
			width:318px;
			height:184px;
			background-image:url(../../images/footer/window_newsletter_bg.jpg);
		}
