@charset "utf-8";
/* CSS Document */

/*************************************************************\
*	generic styling
/*************************************************************/
html * {
	padding:0; margin:0;
}

body {
	font-family:Arial, Helvetica, sans-serif;
	font-size:11px;
	color:#FFF;
	background: #000718 url(../../images/bg-tile.jpg) repeat-x top left;
}

h1 {
	position:absolute; z-index:10; width:264px; height:184px; top:0px; left:345px;
}

h1 a {
	position:relative; display:block; width:264px; height:184px;
	background-image:url(../../images/header/logo_kp.png); background-repeat:no-repeat;
}

/*
*	Competition section headers
*/
h2 {
		text-indent: -9999px; position:relative; display:block; width:715px; height:99px;
		background-repeat:none;
}

	.moment-mara {
		background:url(../../images/competition/batchcode-entry/header_dynamic-vote_maradonna.png);	
	}
	
	
	
	.step2-tellus {
		width:417px; height:54px; background-image:url(../../images/competition/userdata-entry/header_tellus.png);	
	}
	
	h2.captcha {
		width:440px; height:57px; background-image:url(../../images/competition/code-entry/header_securitycode.png);	
	}
	
/**********************************/

h3 {
	text-indent:-9999px; display:block; margin-top:50px;		
}

	h3.range {
		width:291px; height:62px; margin-top:65px;
		background-image:url(../../images/footer/range/range_txt.png);
	}
	
	h3.about {
		width:184px; height:52px; margin-top:63px;
		background-image:url(../../images/footer/about/about_txt.png);
	}

	/*h3.newsletter {
		width:257px; height:48px;
		background-image:url(../../images/footer/newsletter/newsletter_txt.png);
	}*/
	
	h3.thankyou {
		width:254px; height:52px; margin-top:64px;
		background-image:url(../../images/footer/newsletter/thankyou_txt.png);
	}
	
	/* adding footer styling for competition footer
	 * date:   08.04.2010
	 * author: Craig Barber
	*/
	
	h3.prizes {
		width:126px; height:77px; margin-top:35px;
		background-image:url(../../images/footer/prizes/subheader_prizes.png);
	}
	
	h3.league {
		width:129px; height:81px; margin-top:35px;
		background-image:url(../../images/footer/league/subheader_league.png);
	}
	
	h3.newsletter {
		width:220px; height:60px; margin-top:35px;
		background-image:url(../../images/footer/newsletter/subheader_newsletter.png);
	}
	
	
	
a.cta {
	text-indent:-9999px; display:block;		
}

	.range {
		width:118px; height:20px; margin-top:5px;
		background-image:url(../../images/footer/range/range_btn.png);
		background-repeat:no-repeat;
	}
	
	.about {
		width:127px; height:19px; margin-top:9px;
		background-image:url(../../images/footer/about/about_btn.png);
		background-repeat:none;
	}
	
	.league-link {
		width:129px; height:24px;
		background-image:url(../../images/footer/league/btn_league.png);
		background-repeat:none;
	}


p.footer {
	text-indent:-9999px; display:block;	background-repeat:none;
}

	.prizes {
		width:126px; height:59px; background-image:url(../../images/footer/prizes/desc_prizes.png);
	}
	
	.league {
		width:129px; height:31px; background-image:url(../../images/footer/league/desc_league.png);
	}



/*************************************************************\
*	form styling
/*************************************************************/
form {
	margin-top:8px;	width:258px; margin-left:16px;
}

	.comp-process {
		float:left; width: 406px; margin:0;	
	}
	
		.step2 {
			width:452px; margin-left:5px;	
		}
		
		form.step3 {
			width:340px; margin-left:30px; 
		}

p.field {
	padding-bottom:8px;	
}

	p.field span {
		display:block; margin-left:106px; font-size:10px; line-height:22px;
	}

p.field-captcha {
	padding-bottom:2px; overflow:hidden;	
}
	
p.checkbox {
	padding-bottom:0px;	
}

fieldset {
	border:none;
}

	fieldset#checkboxes {
		margin-left:102px;
	}
	
	fieldset#submit {
		margin-left:102px; margin-top:10px;	
	}
	
	fieldset#captcha {
		margin-left:65px;	
	}

label {
	display:block; margin-bottom:3px; background-repeat:no-repeat;	
}

	label.batchcode {
		text-indent:-9999px; width:404px; height:58px;
		background-image:url(../../images/competition/batchcode-entry/label_batchcode.png);
	}
	
	label.moment {
		text-indent:-9999px; width:362px; height:23px;
		background-image:url(../../images/competition/batchcode-entry/label_moment.png);
	}
	
	/*GENERIC TEXT LABEL*/
	label.text {
		float:left; width:105px; height:23px; line-height:23px; margin:2px 0 0 0;
	}
	/*******************/
	
	label.checkbox {
		height:15px; line-height:16px; font-size:10px;
	}
	
	label.captcha {
		height:18px; line-height:18px;	
	}

/*INPUTS*/
input {
	height:20px; padding:3px 3px 0px 3px; border:1px solid #808080; font-size:13px; font-weight:bold; color:#053071;
	
}

input.text {
	width:218px;			
}

input.checkbox {
	width:20px; height:15px; margin-right:5px; border:none; float:left;	
}

input.postcode {
	width:50px; margin-right:4px;	
}

input.captcha {
	width:332px; 	
}


input#email {
	width:252px; height:17px; border:none; font-size:10px;
	background-image:url(../../images/footer/newsletter/input_bg.png);
}

input#batchcode {
	width:215px;
}

input#submit {
	display:block; padding:0; border:none;
}

	input.newsletter {
		width:76px; height:24px; float:right; margin-top:5px;	
	}

	input.next {
		width:101px; height:35px; float:left;	
	}
	
	
/*END OF INPUTS*/


select {
	height:25px; border:1px solid #808080; font-size:13px; font-weight:bold; color:#053071; 	
}

a.back {
	float:left; display:block; text-indent:-9999px;
	width:106px; height:35px; background-image:url(../../images/competition/batchcode-entry/btn_back.png);
	background-repeat:none;
}

#visual {
	float:left;	
}

/*STEP 2 STYLING*/
.content_box {
	position:relative; /* same as content div */
	/*border:1px solid red;*/
}

	.userform {
		width:910px; height:421px; z-index:10; top:0; left:0;
		background:url(../../images/competition/userdata-entry/img_prizes-large.png) no-repeat bottom left;
	}
	
	.form-content {
		float:right; width:452px; height:421px;
	}

img.captcha {
	float:left; border:none; display:block; margin-left:78px;	
}

a.refresh {
	float:left; display:block; font-weight:bold; text-decoration:none; margin-left:20px; margin-top:46px;	
}

	a.refresh:hover {
		text-decoration:underline;	
	}
	
	a.refresh:visited {
		text-decoration:none; color:white;	
	}
	
